#Get html for url, observe partern for each volume, issue and article
papers <- c("https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00001?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00002?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00003?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00004?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00005?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00006?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00007?crawler=true&mimetype=application/pdf", "https://www.ingentaconnect.com/contentone/tpp/pap/2010/00000038/00000001/art00008?crawler=true&mimetype=application/pdf")
save_here <- paste0("file_", 1:8, ".pdf")
mapply(download.file, papers, save_here, mode = "wb")
